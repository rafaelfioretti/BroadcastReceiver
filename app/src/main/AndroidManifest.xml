<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.rafaelfioretti.broadcastreceiver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".receiver.MyReceiver">
            <intent-filter>
                <action android:name="android.app.action.ACTION_PASSWORD_SUCCEEDED" />
            </intent-filter>
        </receiver>
        <receiver android:name=".receiver.MyOrderedReceiver1">
            <intent-filter android:priority="1">
                <action android:name="orderedbroadcast" />
            </intent-filter>
        </receiver>
        <receiver android:name=".receiver.MyOrderedReceiver2">
            <intent-filter android:priority="2">
                //quanto maior a prioridade, 1o ele ser√° executado.
                <action android:name="orderedbroadcast" />
            </intent-filter>
        </receiver>

        <activity android:name=".AlarmeActivity"></activity>

        <receiver android:name=".receiver.AlarmeReceiver"/>
    </application>

</manifest>